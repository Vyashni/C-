//Program to check age of a user is eligible for voting or not
#include<iostream>
using namespace std;
int main()
{
    int age;
    cout<<"Enter your age:";
    cin>>age;
    if(age>=18)
    
        cout<<"\nYou are eligible to vote\n";
    else
    {
        cout<<"\nSorry you are not old enough to vote\n";
    cout<<"\nWait for just "<<18-age<<" more years\n";
    }
        return 0;
}

